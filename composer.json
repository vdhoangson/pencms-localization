{
    "name": "vdhoangson/pencms-localization",
    "description": "Core PenCMS",
    "keywords": ["vdhoangson", "pencms", "core", "cms", "localization", "laravel"],
    "homepage": "https://github.com/vdhoangson/pencms-localization",
    "license": "MIT",
    "authors": [
        {
            "name": "vdhoangson",
            "email": "vdhoangson@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.3"
    },
    "autoload": {
        "psr-4": {
            "vdhoangson\\Localization\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "vdhoangson\\Localization\\Providers\\LocalizationServiceProvider"
            ],
            "aliases": {
                "Localization": "vdhoangson\\Localization\\Facades\\Localization"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
